<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="root" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.metal666.jtc.config.view.ConfigController">
  <columnConstraints>
      <ColumnConstraints hgrow="NEVER" minWidth="-Infinity" />
    <ColumnConstraints hgrow="ALWAYS" />
    <ColumnConstraints hgrow="NEVER" minWidth="-Infinity" />
      <ColumnConstraints hgrow="NEVER" minWidth="-Infinity" />
      <ColumnConstraints hgrow="NEVER" minWidth="-Infinity" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="25.0" prefHeight="25.0" vgrow="NEVER" />
    <RowConstraints minHeight="25.0" prefHeight="25.0" vgrow="NEVER" />
  </rowConstraints>
   <children>
      <RadioButton fx:id="radioButton" mnemonicParsing="false" onAction="#onSelect" />
      <TextField fx:id="nameTextField" editable="false" GridPane.columnIndex="1" />
      <TextField fx:id="paramsTextField" editable="false" GridPane.columnSpan="4" GridPane.rowIndex="1" />
      <Button fx:id="doneButton" defaultButton="true" mnemonicParsing="false" onAction="#onDoneChanging" text="Done" GridPane.columnIndex="2" />
      <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#onCancelChanging" text="Cancel" GridPane.columnIndex="3" />
      <Button fx:id="changeButton" mnemonicParsing="false" onAction="#onChange" text="Change" GridPane.columnIndex="4" />
      <Button mnemonicParsing="false" onAction="#onDelete" text="Delete" GridPane.columnIndex="4" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
   </children>
</GridPane>
